<h1>New Artist Page!!</h1>

<% if flash[:my_errors] %>
    <% flash[:my_errors].each do |error| %>
        <%= error %><br>
        <% end %>
        <% end %>

<%= form_for @artist do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name%><br><br>

    <%= f.label :title%>
    <%= f.text_field :title%><br><br>

    <%= f.label :age%>
    <%= f.text_field :age%><br><br>

    <%= f.submit %>
    <% end %>